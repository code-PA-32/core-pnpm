{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "turbo": "latest",
    "typescript": "~5.2.2",
    "prettier": "~3.0.3",
    "prettier-plugin-sql": "~0.15.1",
    "prettier-plugin-tailwindcss": "~0.5.6",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "@core/core-pnpm",
  "pnpm": {
    "overrides": {
      "graphql": "16.8.1"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "less",
        "webpack",
        "@apollo/server"
      ],
      "allowAny": [
        "typescript"
      ],
      "allowedVersions": {
        "prettier": "3.0.0"
      }
    }
  }
}
